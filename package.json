{
  "main": "dist-electron/main.js",
 
  "scripts": {
    "dev": "npm run build:electron && concurrently \"npm run dev --prefix renderer\" \"npm run build:electron -- --watch\" \"wait-on http://localhost:5173 && nodemon\"",
    "build:electron": "tsc -p tsconfig.electron.json"
  },
  "dependencies": {
    "electron": "^40.2.1"
  },
  "devDependencies": {
    "@types/electron": "^1.6.12",
    "@types/node": "^25.2.3",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.3"
  }
}
